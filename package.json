{
  "name": "webmart-backend",
  "version": "0.0.1",
  "description": "Awesome project developed with TypeORM.",
  "main": "dist/server.js",
  "type": "commonjs",
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/cookie": "^0.5.1",
    "@types/express": "^4.17.17",
    "@types/multer": "^1.4.7",
    "@types/node": "^16.11.10",
    "nodemon": "^2.0.21",
    "ts-node": "10.7.0",
    "typescript": "4.5.2"
  },
  "dependencies": {
    "@nestjs/typeorm": "^8.0.3",
    "@types/compression": "^1.7.2",
    "@types/cookie-parser": "^1.4.3",
    "@types/cors": "^2.8.13",
    "@types/jsonwebtoken": "^9.0.1",
    "@types/lodash": "^4.14.191",
    "@types/nodemailer": "^6.4.7",
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "dotenv": "^16.0.3",
    "envalid": "^7.3.1",
    "express": "^4.18.2",
    "express-validation": "^4.1.0",
    "googleapis": "^111.0.0",
    "helmet": "^6.0.1",
    "jsonwebtoken": "^9.0.0",
    "moment": "^2.29.4",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^6.9.1",
    "pg": "^8.4.0",
    "rand-token": "^1.0.1",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^4.1.2",
    "typeorm": "^0.2.45",
    "typeorm-naming-strategies": "^4.1.0",
    "winston": "^3.8.2",
    "winston-daily-rotate-file": "^4.7.1"
  },
  "scripts": {
    "start": "pm2 start webmart.json",
    "stop": "pm2 stop webmart.json",
    "restart": "pm2 startOrRestart webmart.json --update-env",
    "reload": "pm2 startOrReload webmart.json --update-env",
    "ts-node": "node -r ts-node/register/transpile-only src/server.ts",
    "dev": "nodemon",
    "typeorm": "ts-node ./node_modules/typeorm/cli.js --config src/config/postgreSeedTypeorm.ts",
    "typeorm:migrate": "npm run typeorm -- migration:run",
    "typeorm:seed": "ts-node ./node_modules/typeorm/cli.js --config src/config/postgreSeedTypeorm.ts migration:run",
    "typeorm:seed:undo": "ts-node ./node_modules/typeorm/cli.js --config src/config/postgreSeedTypeorm.ts migration:revert",
    "prebuild": "rimraf dist/*",
    "build": "tsc",
    "build:check": "tsc --noEmit"
  }
}
